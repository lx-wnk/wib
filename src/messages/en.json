{
  "command": {
    "list": {
      "option": {
        "yesterday": {
          "description": "List yesterday",
          "flag": "-y, --yesterday"
        },
        "full": {
          "description": "List the full data (unshortened)",
          "flag": "-f, --full"
        },
        "order": {
          "description": "Order by (time, key, value, id)",
          "flag": "-o, --order <key>"
        },
        "day": {
          "description": "List a specific date",
          "flag": "-d, --day <day>"
        }
      },
      "description": "Show the report",
      "execution": {
        "invalidOrder": "The provided order key is invalid: "
      }
    },
    "note": {
      "option": {
        "delete": {
          "description": "Delete a specified note",
          "flag": "-d, --delete <key>"
        },
        "edit": {
          "description": "Edit a specified note",
          "flag": "-e, --edit <key>"
        }
      },
      "description": "Handle notes",
      "execution": {
        "create": "Created new note with value: {{value}}",
        "edit": "Edited note with id: {{id}}",
        "couldNotEdit": "Could not edit note with id: {{id}}",
        "delete": "Deleted note with id: {{id}}",
        "couldNotDelete": "Could not delete note with id: {{id}}",
        "missingOptions": "Please provide a value for the note `wib note <key> <value>`"
      }
    },
    "rest": {
      "description": "Add a new rest",
      "option": {
        "edit": {
          "description": "Change the rest ending to now or by -t to a specified time",
          "flag": "-e, --edit <key>"
        },
        "time": {
          "description": "Create a rest with specified time",
          "flag": "-t, --time <hour:minute>"
        }
      },
      "execution": {
        "create": "Rest lastet {duration} and ended at: ",
        "couldNotEdit": "Could not edit rest with id: "
      }
    },
    "start": {
      "description": "Set the start time for the day"
    },
    "stop": {
      "description": "Set the stop time for the day"
    },
    "worklog": {
      "option": {
        "delete": {
          "description": "Delete a specified worklog",
          "flag": "-d, --delete <key>"
        },
        "edit": {
          "description": "Edit a specified worklog",
          "flag": "-e, --edit <key>"
        },
        "time": {
          "description": "Specify the finish time",
          "flag": "-t, --time <hour:minute>"
        },
        "unexpected": {
          "description": "Add unexpected informations for this worklog",
          "flag": "-u, --unexpected <your information here> (sadly currenlty only a string is support - please tick your data between \" or ')"
        }
      },
      "description": "Handle worklogs",
      "execution": {
        "create": "Worklog {{iterator}} with key[{{key}}] and value[{{value}}] was created at {{time}}",
        "updated": "Worklog {{iterator}} with new values key[{{key}}] and value[{{value}}] was updated",
        "deleted": "Worklog {{iterator}} was deleted",
        "couldNotEdit": "Could not edit worklog with id: ",
        "delete": "Deleted worklog with id: ",
        "couldNotDelete": "Could not delete worklog with id: ",
        "missingOptions": "Please provide a value for the worklog `wib track <key> <value>`",
        "invalidTime": "The provided time is invalid! Please adjust your input"
      }
    },
    "migrate": {
      "option": {
        "data": {
          "description": "Migrate your existing data",
          "flag": "-d, --data"
        }
      },
      "description": "Migrate data from filesystem to sqlite"
    }
  },
  "format": {
    "list": {
      "keys": {
        "day": "Report for date",
        "start": "Clocked in",
        "stop": "Clocked out",
        "note": "Note({{iterator}}) [{{time:datetime}}]",
        "worklog": "Worklog({{iterator}}) [{{time:time}}]",
        "rest": "Break"
      },
      "values": {
        "day": "{{start:date}}",
        "start": "{{start:time}}",
        "stop": "{{finish:time}}",
        "note": "{{value}}",
        "worklog": "{{duration:duration}} {{key}} {{value}}",
        "rest": "{{duration:duration}} Break duration"
      }
    },
    "commandResponse": {
      "start": "You clocked in at: {{start:time}}",
      "stop": "You clocked out at: {{finish:time}}"
    },
    "break": {
      "key": {
        "type": "Break"
      },
      "value": {
        "duration": "Break duration"
      }
    },
    "day": {
      "key": "Report for date"
    },
    "invalid": "INVALID FORMAT provided: ",
    "note": {
      "key": {
        "type": ""
      }
    },
    "start": {
      "key": "Clocked in"
    },
    "stop": {
      "key": {
        "unset": "Estimated clock out",
        "set": "Clocked out"
      }
    },
    "time": {
      "hour": "h",
      "minute": "m"
    },
    "worklog": {
      "key": {
        "type": "Worklog"
      },
      "commandResponse": {
      }
    },
    "workDuration": {
      "key": "Worked time"
    },
    "breakDuration": {
      "key": "Break duration"
    }
  }
}
