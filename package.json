{
  "name": "wib",
  "version": "0.5.0",
  "description": "Time tracking tool for the cli",
  "keywords": [
    "tracker",
    "cli",
    "time tracking"
  ],
  "author": "Alexander Wink <wib@jinnoflife.com> (https://github.com/jinnoflife)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinnoflife/wib.git"
  },
  "bugs": {
    "url": "https://github.com/jinnoflife/wib/issues"
  },
  "homepage": "https://github.com/jinnoflife/wib#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/Wib.ts",
  "directories": {
    "bin": "./bin",
    "dist": "./dist",
    "lib": "./src",
    "test": "./tests"
  },
  "bin": {
    "wib": "./bin/index.js"
  },
  "dependencies": {
    "commander": "^7.0.0",
    "inversify": "^5.0.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.0.0",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.40",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-unused-imports": "^0.1.3",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "sinon": "^9.0.3",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "command:exec:start": "npm run wib",
    "command:exec:rebuild": "npm run tsc && npm run wib",
    "style:lint": "./node_modules/.bin/eslint src tests --ext .js,.jsx,.ts,.tsx",
    "style:fix": "./node_modules/.bin/eslint src tests --fix --ext .js,.jsx,.ts,.tsx",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "typeorm:migration:create": "npm run typeorm:cli -- migration:create -n",
    "typeorm:migration:generate": "npm run typeorm:cli -- migration:generate -n",
    "build": "rm -rf ./dist; npm run tsc",
    "tsc": "./node_modules/.bin/tsc",
    "dev": "./node_modules/.bin/tsc --watch & node dist",
    "wib": "node ./bin/index.js",
    "test": "TZ=UTC ./node_modules/.bin/mocha -r ts-node/register",
    "coverage": "./node_modules/.bin/nyc npm test",
    "link": "npm run build && npm link --force"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  }
}
